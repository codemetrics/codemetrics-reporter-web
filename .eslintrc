{
    "rules": {
        "indent": [
            1,
            2
        ],
        "quotes": [
            1,
            "double"
        ],
        "linebreak-style": [
            1,
            "unix"
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules":true
    },
    "plugins": [
        "react"
    ]
}